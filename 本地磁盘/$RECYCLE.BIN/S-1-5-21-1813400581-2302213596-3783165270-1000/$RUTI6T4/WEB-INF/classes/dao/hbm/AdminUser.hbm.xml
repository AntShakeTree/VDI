<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tibetone.ztkjhim.bean.AdminUser" table="AdminUser_" catalog="dlgs">

		<id name="aid" type="long" column="aid">
		  <generator class="increment"></generator>
		</id>
		
		<property name="aname" column="aname_" type="string" length="25"></property>
		<property name="apass" column="apass_" type="string" length="12"></property>
		<property name="asta" column="asta_" type="string" length="3"></property>
		
		</class>
</hibernate-mapping>