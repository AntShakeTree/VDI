<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tibetone.ztkjhim.bean.Business" table="Business_"
		catalog="dlgs">

		<id name="id" type="long" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="code" column="code_" type="string" length="5"></property>
		<property name="desc" column="desc_" type="string"
			length="1000">
		</property>
		<property name="openOrClose" column="openOrClose_"
			type="boolean">
		</property>
		<!--  
	<set name="customers" table="Business_customer_" cascade="all-delete-orphan" inverse="false" lazy="true" order-by="id desc">
			<key column="businessid_"></key>
				<many-to-many class="com.tibetone.ztkjhim.bean.Customer" />
			</set>-->

	</class>
</hibernate-mapping>