<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tibetone.ztkjhim.bean.AuditRecord" table="AUDIT_LOGS"
		catalog="dlgs">

		<id  type="long" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="entityId" column="entity_Id_" type="integer" length="5"></property>
		<property name="entiyClass" column="entiy_Class_" type="string"
			length="300">
		</property>
			<property name="createDate" column="create_date_" type="date">
		</property>
	</class>
</hibernate-mapping>